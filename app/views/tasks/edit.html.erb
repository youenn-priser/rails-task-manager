<h1>Edit task <%= @task.title %></h1>
<%= form_for(@task) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_area :details %>
  <%= f.label :completed %>
  <%= f.check_box :completed, checked_value: true, unchecked_value: false %>
  <%= f.submit %>
<% end %>
